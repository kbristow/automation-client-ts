    "async-exit-hook": "^2.0.1",
    "axios": "^0.17.1",
    "base-64": "^0.1.0",
    "body-parser": "^1.18.2",
    "child-process-promise": "^2.2.1",
    "config": "^1.28.1",
    "continuation-local-storage": "^3.2.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fs-extra": "^5.0.0",
    "github": "^13.0.1",
    "glob-stream": "^6.1.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.6.0",
    "helmet": "^3.9.0",
    "inquirer": "^4.0.1",
    "isbinaryfile": "^3.0.2",
    "isomorphic-fetch": "^2.2.1",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.4",
    "lru_map": "^0.3.3",
